{
  "name": "@bollo-aggrey/ts-autogen",
  "version": "0.1.6",
  "description": "Lightweight Lombok-like decorators for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "homepage": "https://github.com/bollo-omar/ts-autogen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bollo-omar/ts-autogen.git"
  },
  "bugs": {
    "url": "https://github.com/bollo-omar/ts-autogen/issues"
  },
  "bin": {
    "ts-autogen-check": "dist/cli/version-check.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "NODE_OPTIONS='--import tsx' vitest run",
    "test:watch": "NODE_OPTIONS='--import tsx' vitest watch",
    "check-updates": "node dist/cli/version-check.js --check-updates",
    "postinstall": "node scripts/postinstall.js",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "scripts",
    "README.md"
  ],
  "author": {
    "name": "Aggrey Bollo",
    "email": "bolloaggrey@gmail.com",
    "url": "https://github.com/bollo-omar"
  },
  "keywords": [
    "typescript",
    "decorators",
    "lombok",
    "autogen",
    "boilerplate",
    "getters",
    "setters",
    "builder",
    "data",
    "toString",
    "equals",
    "immutable",
    "annotations"
  ]
}
